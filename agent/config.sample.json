{
  "agent": {
    "feed_interval_ms": 100,
    "max_queue_size": 1000,
    "processing_timeout": 5.0,
    "enable_logging": true,
    "use_anthropic": false,
    "scenario": "general"
  },
  "mcp": {
    "default_timeout": 10.0,
    "max_retries": 3,
    "enable_caching": true,
    "custom_tools": {}
  },
  "camera": {
    "expected_fps": 30,
    "frame_buffer_size": 10,
    "segment_validation": true,
    "min_confidence_threshold": 0.5,
    "max_segments_per_frame": 100
  },
  "modal": {
    "cpu_count": 2,
    "memory_mb": 2048,
    "timeout_seconds": 300,
    "max_concurrent": 10,
    "scale_down_delay": 60.0
  },
  "anthropic": {
    "api_key": "",
    "model": "claude-3-sonnet-20240229",
    "max_tokens": 4096,
    "temperature": 0.7,
    "timeout": 30.0,
    "max_retries": 3
  },
  "prompts": {
    "use_custom_prompts": true,
    "prompt_directory": "./prompts",
    "default_scenario": "general",
    "enable_prompt_caching": true
  },
  "environment": "development",
  "debug_mode": false
}
